{
	"version": 2,
	"builds": [{
		"src": "index.js",
		"use": "@vercel/node"
	}],
	"routes": [{
			"src": "/carros/new",
			"dest": "/carros/new"
		},
		{
			"src": "/carros/create",
			"methods": ["POST"],
			"dest": "/carros/create"
		},
		{
			"src": "/carros/:id",
			"dest": "/carros/:id"
		},
		{
			"src": "/carros/delete",
			"methods": ["POST"],
			"dest": "/carros/delete"
		},
		{
			"src": "/carros",
			"dest": "/carros"
		},
		{
			"src": "/(.*)",
			"dest": "/"
		},


		{
			"src": "/endereco_lojas/new",
			"dest": "/endereco_loja/new"
		},
		{
			"src": "/endereco_lojas/create",
			"methods": ["POST"],
			"dest": "/endereco_lojas/create"
		},
		{
			"src": "/endereco_lojas/:id",
			"dest": "/endereco_lojas/:id"
		},
		{
			"src": "/endereco_lojas/delete",
			"methods": ["POST"],
			"dest": "/endereco_lojas/delete"
		},
		{
			"src": "/endereco_lojas",
			"dest": "/endereco_lojas"
		},

    
    {
			"src": "/clientes/new",
			"dest": "/clientes/new"
		},
		{
			"src": "/clientes/create",
			"methods": ["POST"],
			"dest": "/clientes/create"
		},
		{
			"src": "/clientes/:id",
			"dest": "/clientes/:id"
		},
		{
			"src": "/clientes/delete",
			"methods": ["POST"],
			"dest": "/clientes/delete"
		},
		{
			"src": "/clientes",
			"dest": "/clientes"
		}

	]
}